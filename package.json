{
  "name": "node-mime-types",
  "version": "1.0.0",
  "author": "Adrien Valcke <adrienvalcke@icloud.com> (https://github.com/adrienv1520)",
  "description": "A Node.js and zero-dependencies MIME type utility",
  "bin": {},
  "main": "src/index.js",
  "scripts": {
    "build": "node build/index.js",
    "postbuild": "npm run format && npm run lint:fix",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "mocha test/* --exit"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adrienv1520/node-mime-types.git"
  },
  "engines": {
    "node": ">=8.17.0",
    "npm": ">=6.13.4"
  },
  "keywords": [
    "node-mime-types",
    "MIME",
    "type",
    "file",
    "file name",
    "path"
  ],
  "files": [
    "src"
  ],
  "dependencies": {},
  "devDependencies": {
    "chai": "4.3.6",
    "eslint": "8.15.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "lodash": "4.17.21",
    "mime-db": "1.52.0",
    "mocha": "10.0.0",
    "prettier": "2.6.2"
  },
  "private": false,
  "preferGlobal": false,
  "analyze": false,
  "license": "MIT"
}
